{
  "name": "js-reactify",
  "version": "0.1.1",
  "license": "LGPL-3.0",
  "homepage": "https://github.com/js-works/js-reactify",
  "main": "dist/js-reactify.min.js",
  "types": "dist/types",
  "repository": {
    "type": "git",
    "url": "https://github.com/js-works/js-reactify.git"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/node": "^8.0.0",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "awesome-typescript-loader": "^5.0.0",
    "chai": "^4.1.2",
    "compression-webpack-plugin": "^1.1.11",
    "cross-conf-env": "^1.1.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.1.1",
    "react": "^16.3.2",
    "shx": "^0.2.2",
    "ts-loader": "^4.2.0",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
  "scripts": {
    "clean": "node_modules/.bin/shx rm -rf ./build ./dist",
    "build-dev": "webpack --config webpack.config.dev.js",
    "build-prod": "webpack --config webpack.config.prod.js",
    "test": "node_modules/.bin/mocha -r ts-node/register src/test/**/*.ts",
    "test-watch": "node_modules/.bin/mocha -w -r ts-node/register src/test/**/*.ts",
    "docs": "node_modules/.bin/shx rm -rf ./dist/docs && \"node_modules/.bin/cross-conf-env\" typedoc --options typedoc.json src/main/api/ -name \"js-rectify vnpm_package_version\"",
    "demo": "webpack-dev-server --config ./webpack.config.demo.js --watch --open",
    "dist": "npm run clean && npm run test && npm run build-dev && npm run build-prod && npm run docs"
  },
  "dependencies": {
    "js-spec": "^0.1.23",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  }
}
